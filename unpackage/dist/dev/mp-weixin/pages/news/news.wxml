<view><news-tab-bar vue-id="1" tabList="{{tabList}}" tabIndex="{{tabIndex}}" border="{{false}}" data-event-opts="{{[['^changeTab',[['changeTab']]]]}}" bind:changeTab="__e" bind:__l="__l"></news-tab-bar><view class="uni-tab-bar"><swiper class="swiper-box" style="{{'height:'+(swiperheight+'px')+';'}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['tabChange',['$event']]]]]}}" bindchange="__e"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadmore']]]]}}" bindscrolltolower="__e"><block wx:for="{{newslist.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><common-list vue-id="{{'2-'+index}}" item="{{item}}" index="{{index}}" bind:__l="__l"></common-list></block></block><load-more vue-id="3" loadtext="{{newslist.loadtext}}" bind:__l="__l"></load-more></scroll-view></swiper-item><swiper-item><scroll-view class="list" scroll-y><view class="search-style"><input class="uni-input" placeholder-class="icon iconfont icon-sousuo topic-style" placeholder="搜索内容"/></view><swiper class="topic-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{topic.swiper}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item><image src="{{item.src}}"></image></swiper-item></block></block></swiper><topic-nav vue-id="4" nav="{{topic.nav}}" bind:__l="__l"></topic-nav><topic-news vue-id="5" topic="{{topic.list}}" bind:__l="__l"></topic-news></scroll-view></swiper-item></swiper></view></view>